{
  "name": "mantis-ui",
  "version": "0.0.1",
  "private": true,
  "description": "Mantis UI",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14.18.0",
    "pnpm": ">=6.23.0"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint --ext .tsx,.ts,.js,.jsx src",
    "lint:fix": "eslint --ext .tsx,.ts,.js,.jsx src --fix",
    "pnpm:setup": "newt exec npm install -g pnpm",
    "prepare": "husky install",
    "serve": "vite preview --open --port 3000",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mantis-ui"
  ],
  "author": "Santosh Kalidindi <skalidindi@netflix.com>",
  "contributors": [],
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^26.2.0",
    "@ag-grid-community/core": "^26.2.1",
    "@ag-grid-community/react": "^26.2.0",
    "@ant-design/icons": "^4.7.0",
    "ahooks": "^3.1.9",
    "antd": "^4.18.5",
    "date-fns": "^2.28.0",
    "ky": "^0.28.7",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.2.2",
    "react-location": "^3.3.4",
    "react-query": "^3.34.12",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^1.1.4",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.3",
    "postcss": "^8.4.6",
    "postcss-nesting": "^10.1.2",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-imp": "^2.1.3",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@ag-grid-community/client-side-row-model",
        "@ag-grid-community/react",
        "@ant-design/icons",
        "@sentry/browser",
        "@sentry/integrations",
        "antd",
        "antd/es/alert/style",
        "antd/es/breadcrumb/style",
        "antd/es/button/style",
        "antd/es/card/style",
        "antd/es/checkbox/style",
        "antd/es/empty/style",
        "antd/es/form/style",
        "antd/es/input/style",
        "antd/es/menu/style",
        "antd/es/message/style",
        "antd/es/notification/style",
        "antd/es/result/style",
        "antd/es/spin/style",
        "antd/es/statistic/style",
        "antd/es/switch/style",
        "antd/es/tabs/style",
        "antd/es/tag/style",
        "antd/es/upload/style",
        "date-fns",
        "ky",
        "react",
        "react-dom",
        "react-dom/server",
        "react-error-boundary",
        "react-helmet-async",
        "react-location",
        "react-query",
        "web-vitals"
      ]
    }
  }
}
